function albums(album) {
  const event=([])

  for( let i=0; i<album.length; i++) {
    event.push({
        title: `${album[i].albumname} ðŸ“€`,
        date: album[i].Releasedate,
        color: album[i].color,
        textColor: album[i].fontcolor
    })
  }

  return event
}

export default albums